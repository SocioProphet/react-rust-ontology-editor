{
  "name": "react_rust_ontology_editor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "build-wasm": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "rs",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500
    },
    "build-bindgen": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js, rs",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500
    },
    "build": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "rs",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-wasm": "cargo +nightly build --target wasm32-unknown-unknown",
    "build-bindgen": "wasm-bindgen target/wasm32-unknown-unknown/debug/react_rust_ontology_editor.wasm --out-dir build",
    "build": "npm run build-wasm && npm run build-bindgen && npx webpack",
    "watch": "npm-watch",
    "serve": "webpack-dev-server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "d3": "4.13.0",
    "npm-watch": "^0.3.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.0"
  }
}
